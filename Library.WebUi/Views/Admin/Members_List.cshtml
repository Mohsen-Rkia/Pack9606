@model IEnumerable<Library.Model.Member>
@{
    ViewBag.Title = "لیست اعضا";
}

@if (TempData["Message"] != null)
            {
    <div id="Message" class="col-md-12">
        <div class="alert alert-success text-center">
            <h2>@TempData["Message"]</h2>
        </div>
    </div>
}

<h2>لیست اعضا</h2>
<div class="row">
    <div style="margin:10px;" class="col-md-2">
        <a href="http://localhost:9099/Admin/AddMember" class="btn btn-info btn-block">
            افزودن عضو
        </a>
    </div>
</div>
<table class="table table-hover table-bordered table-striped">
    <thead>
        <tr>
            <th>شماره اعضا</th>
            <th>نام</th>
            <th>نام خانوادگی</th>
            <th>نام کاربری</th>
            <th>آدرس ایمیل</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.Id</td>
                <td>@member.Name</td>
                <td>@member.LastName</td>
                <td>@member.UserName</td>
                <td>@member.Email</td>
            </tr>
        }
    </tbody>

</table>


