@model Library.Model.RequestBook

@{
    ViewBag.Title = "حذف درخواست";
}

<div class="panel-heading">
    <h3 class="panel-title">حذف کاربر</h3>
</div>

<div class="panel-body">

    <h3>آیا از حذف این درخواست اطمینان دارید؟</h3>
    <div>

        <hr />
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="حذف" class="btn btn-default" /> |
                <span class="btn btn-primary">@Html.ActionLink("بازگشت", "Index")</span>
            </div>
        }
    </div>
</div>
