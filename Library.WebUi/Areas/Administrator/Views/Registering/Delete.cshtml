@model Library.Model.Member

@{
    ViewBag.Title = "حذف";
}

<div class="panel-heading">
    <h3 class="panel-title">حذف کاربر</h3>
</div>

<div class="panel-body">
    <h3>آیا از حذف @Model.UserName اطمینان دارید؟</h3>
    <div>
        <hr />


        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="حذف" class="btn btn-default" /> |
                <span class="btn btn-primary">@Html.ActionLink("بازگشت", "Members_List", "Admin")</span>

            </div>
        }
    </div>
</div>

